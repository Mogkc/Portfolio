<!-- Start of container -->
<div class="container">
  <main class="row">
    <div class="col-md-11">
      <article class="block">
        <h1 class="block-header">Portfolio</h1>
        <hr />
        <h3>Group Projects</h3>
        <div class="row" id="group-projects">
          {{#each group}}
          <div class="col-sm-6 col-md-12 col-lg-6">
            <a href="{{github}}">
              <img class="img-fluid port-image" src="{{img}}" alt="{{name}} Group Project">
            </a>
          </div>
          {{/each}}
          <h3>Individual Projects</h3>
          <div class="row">
            {{#each personal}}
            <div class="col-sm-6 col-md-12 col-lg-6">
              <a href="{{github}}">
                <img class="img-fluid port-image" src="{{img}}" alt="{{name}}">
              </a>
            </div>
            {{/each}}
          </div>
      </article>
    </div>
  </main>
</div>
<!-- End of container -->